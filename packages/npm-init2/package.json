{
  "name": "npm-init2",
  "version": "1.0.110",
  "private": false,
  "description": "a lazy npm init for create new package, support yarn workspace. make we without use `mkdir xxx && cd xxx && [npm|yarn] init` with `npx npm-init2 xxx`",
  "keywords": [
    "cli",
    "init",
    "npm",
    "tool",
    "util",
    "yarn-tool",
    "workspace",
    "workspaces",
    "yarn"
  ],
  "homepage": "https://github.com/bluelovers/npm-init2#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/npm-init2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/npm-init2.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "contributors": [],
  "main": "index.js",
  "bin": {
    "npm-init2": "bin/npm-init2.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild:lockfile": "npx sync-lockfile .",
    "npm:publish": "npm publish",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly": "yarn run ncu && yarn run sort-package-json",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" . & git push & echo postpublish",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort"
  },
  "config": {},
  "resolutions": {},
  "dependencies": {
    "@yarn-tool/find-root": "^1.0.24",
    "@yarn-tool/pkg-git-info": "^1.0.8",
    "@yarn-tool/static-file": "^1.0.36",
    "@yarn-tool/update-notifier": "^1.0.13",
    "cross-spawn-extra": "^2.1.0",
    "find-yarn-workspace-root2": "^1.2.19",
    "fs-extra": "^9.0.1",
    "json5": "^2.1.3",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "npm-package-json-loader": "^1.0.29",
    "sort-package-json-scripts": "^1.1.2",
    "validate-npm-package-name": "^3.0.0",
    "workspaces-config": "^1.0.15",
    "yargs": "^15.3.1"
  },
  "bundleDependencies": [],
  "preferGlobal": true,
  "publishConfig": {},
