{
  "name": "@yarn-tool/yarnlock-diff",
  "version": "2.0.0",
  "description": "given one or many old yarn.lock, and one or many new yarn.lock, compute the diff for v1 and berry",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces/tree/master/packages/@yarn-tool/yarnlock-diff#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git"
  },
  "license": "MIT",
  "author": "TychoTa @tychota",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "preversion": "yarn run test",
    "prepublishOnly_": "echo prepublishOnly",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "yarn-tool ncu -u",
    "sort-package-json": "yarn-tool sort"
  },
  "dependencies": {
    "@bluelovers/deep-diff": "^1.0.2",
    "@yarn-tool/semver-diff": "^2.0.0",
    "@yarn-tool/table": "^2.0.0",
    "@yarn-tool/yarnlock-parse": "^2.0.0",
    "@yarn-tool/yarnlock-util": "^2.0.0",
    "@yarnpkg/parsers": "^2.5.1",
    "array-hyper-unique": "^2.0.8",
    "debug-color2": "^1.3.1",
    "npm-package-arg": "^9.1.0",
    "semver": "^7.3.7",
    "ts-type": "^2.1.8",
    "tslib": "^2"
  },
  "devDependencies": {
    "just-diff": "^5.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
