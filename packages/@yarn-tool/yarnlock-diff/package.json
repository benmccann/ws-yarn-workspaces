{
  "name": "@yarn-tool/yarnlock-diff",
  "version": "1.0.0",
  "description": "given one or many old yarn.lock, and one or many new yarn.lock, compute the diff",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces/tree/master/packages/@yarn-tool/yarnlock-diff#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git"
  },
  "license": "MIT",
  "author": "TychoTa @tychota",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "bin": {
    "yarn-lock-diff": "bin/run"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "preversion": "yarn run test",
    "prepublishOnly_": "echo prepublishOnly",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "yarn-tool ncu -u",
    "sort-package-json": "yarn-tool sort"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.1",
    "deep-diff": "^1.0.2",
    "fp-ts": "^2.7.1"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}