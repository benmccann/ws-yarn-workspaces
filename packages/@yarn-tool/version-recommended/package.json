{
  "name": "@yarn-tool/version-recommended",
  "version": "1.0.4",
  "description": "bump new version",
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces/tree/master/packages/@yarn-tool/version-recommended#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "scripts": {
    "lint": "ynpx --quiet eslint -- **/*.ts",
    "test": "echo \"Error: no test specified\"",
    "test:jest": "ynpx --quiet jest -- --coverage",
    "test:mocha": "ynpx --quiet -p ts-node -p mocha mocha -- --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "prepublishOnly": "yarn run prepublishOnly:check-bin && yarn run test",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "ynpx --quiet yarn-tool -- ncu -u",
    "sort-package-json": "ynpx --quiet yarn-tool -- sort"
  },
  "dependencies": {
    "@ts-type/package-dts": "^1.0.43",
    "@yarn-tool/find-root": "^1.0.34",
    "@yarn-tool/workspaces-project": "^1.0.27",
    "semver": "^7.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
