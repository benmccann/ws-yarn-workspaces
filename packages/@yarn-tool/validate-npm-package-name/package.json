{
  "name": "@yarn-tool/validate-npm-package-name",
  "version": "3.0.0",
  "description": "Give me a string and I'll tell you if it's a valid npm package name",
  "keywords": [
    "npm",
    "package",
    "names",
    "validation"
  ],
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces/tree/master/packages/@yarn-tool/validate-npm-package-name#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git"
  },
  "license": "ISC",
  "author": "zeke",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "preversion": "yarn run prepublishOnly",
    "prepublishOnly": "yarn run test",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "yarn-tool ncu -u",
    "sort-package-json": "yarn-tool sort"
  },
  "dependencies": {
    "@yarn-tool/is-builtin-module": "^1.0.0"
  },
  "devDependencies": {}
}
