    "debug-color2": "^1.3.2",
    "fs-extra": "^10.1.0",
    "semver": "^7.3.7",
    "ts-type": "^2.1.10",
    "tslib": ">=2",
    "upath2": "^3.1.15"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "40676553ac196265d354e537459d4c78d67f0a8c"
