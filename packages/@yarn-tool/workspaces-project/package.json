{
  "name": "@yarn-tool/workspaces-project",
  "version": "1.0.1",
  "description": "mixin yarn/lerna workspaces",
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces/tree/master/packages/@yarn-tool/workspaces-project#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "scripts": {
    "lint": "ynpx --quiet eslint -- **/*.ts",
    "test": "echo \"Error: no test specified\"",
    "test:jest": "ynpx --quiet jest -- --coverage",
    "test:mocha": "ynpx --quiet -p ts-node -p mocha mocha -- --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "prepublishOnly": "yarn run prepublishOnly:check-bin && yarn run test",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "ynpx --quiet yarn-tool -- ncu -u",
    "sort-package-json": "ynpx --quiet yarn-tool -- sort"
  },
  "dependencies": {
    "@lerna/project": "^3.21.0",
    "@ts-type/package-dts": "^1.0.26",
    "@types/npm-package-arg": "*",
    "@yarn-tool/changelog": "^1.0.8",
    "find-yarn-workspace-root2": "^1.2.17",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.15",
    "path-dir-normalize": "^1.0.11",
    "sort-object-keys2": "^2.0.3",
    "ts-type": "^1.2.23",
    "upath2": "^3.0.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "40676553ac196265d354e537459d4c78d67f0a8c"
}
