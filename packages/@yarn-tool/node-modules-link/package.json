{
  "name": "@yarn-tool/node-modules-link",
  "version": "1.0.27",
  "description": "link package to node_modules",
  "keywords": [],
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces/tree/master/packages/@yarn-tool/node-modules-link#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "bin": {
    "node-modules-link": "bin/node-modules-link.js"
  },
  "scripts": {
    "lint": "ynpx --quiet eslint -- **/*.ts",
    "test": "echo \"Error: no test specified\"",
    "test:jest": "ynpx --quiet jest -- --coverage",
    "test:mocha": "ynpx --quiet -p ts-node -p mocha mocha -- --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "preversion": "yarn run test",
    "prepublishOnly": "echo prepublishOnly",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "yarn-tool ncu -u",
    "sort-package-json": "yarn-tool sort"
  },
  "dependencies": {
    "@ts-type/package-dts": "^1.0.51",
    "@yarn-tool/find-root": "^1.0.51",
    "find-yarn-workspace-root2": "^1.2.32",
    "fs-extra": "^10.0.0",
    "fs-symlink-extra": "^1.0.6",
    "tslib": "^2.3.1",
    "upath2": "^3.1.10",
    "yargs": "^17.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
