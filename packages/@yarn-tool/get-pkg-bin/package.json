{
  "name": "@yarn-tool/get-pkg-bin",
  "version": "2.0.3",
  "description": "get bin script from package.json",
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces/tree/master/packages/@yarn-tool/get-pkg-bin#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git",
    "directory": "packages/@yarn-tool/get-pkg-bin"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "test": "ts-node test/test.ts",
    "test:jest": "jest --passWithNoTests",
    "test:jest:snapshot": "yarn run test:jest -- -u",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "test:snapshot": "yarn run test -- -u",
    "npm:publish": "npm publish",
    "preversion": "yarn run test",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "@ts-type/package-dts": "^2.0.5",
    "@yarn-tool/resolve-package": "^2.0.3",
    "tslib": ">=2",
    "upath2": "^3.1.15"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "40676553ac196265d354e537459d4c78d67f0a8c"
}
