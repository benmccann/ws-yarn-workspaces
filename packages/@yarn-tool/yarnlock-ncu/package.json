{
  "name": "@yarn-tool/yarnlock-ncu",
  "version": "2.0.4",
  "description": "update yarnlock v1 and berry",
  "keywords": [
    "yarn",
    "yarn-tool",
    "npm",
    "lerna",
    "workspaces",
    "pkg",
    "dependencies",
    "packages",
    "package"
  ],
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces/tree/master/packages/@yarn-tool/yarnlock-ncu#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "scripts": {
    "lint": "ynpx --quiet eslint -- **/*.ts",
    "test": "jest",
    "test:jest": "ynpx --quiet jest -- --coverage",
    "test:mocha": "ynpx --quiet -p ts-node -p mocha mocha -- --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "preversion": "yarn run test",
    "prepublishOnly": "echo prepublishOnly",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "ncu": "yarn-tool ncu -u",
    "sort-package-json": "yarn-tool sort"
  },
  "dependencies": {
    "@yarn-tool/pkg-version-query": "^2.0.4",
    "@yarn-tool/semver-diff": "^2.0.3",
    "@yarn-tool/table": "^2.0.3",
    "@yarn-tool/yarnlock-diff": "^2.0.3",
    "@yarn-tool/yarnlock-entries": "^2.0.2",
    "debug-color2": "^1.3.1",
    "semver": "^7.3.7",
    "tslib": "^2"
  },
  "publishConfig": {
    "access": "public"
  }
}
