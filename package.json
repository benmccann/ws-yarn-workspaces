{
  "name": "ws-create-yarn-workspaces",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "workspaces": [
    "packages/@yarn-tool/*",
    "packages/*"
  ],
  "scripts": {
    "lerna:publish": "npx lerna publish",
    "lerna:publish:yes": "yarn run prepublish:lockfile && npx lerna publish --yes --cd-version patch",
    "ncu": "npx yarn-tool ncu -u",
    "prepublish:lockfile": "ynpx sync-lockfile",
    "postpublish": "yarn run postpublish:subrepo",
    "postpublish:subrepo": "git subrepo push --all",
    "sort-package-json": "npx yarn-tool sort",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "resolutions": {},
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^8.1.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "gitlog": "^3.2.0"
  },
  "peerDependencies": {
    "lerna": "^3"
  }
}
