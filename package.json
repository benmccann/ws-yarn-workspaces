{
  "name": "ws-create-yarn-workspaces",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "yarn",
    "yarn-tool",
    "npm",
    "lerna",
    "workspaces",
    "pkg",
    "dependencies",
    "packages",
    "package"
  ],
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "workspaces": [
    "packages/@yarn-tool/*",
    "packages/@lazy-node/*",
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lerna:publish": "npx lerna publish",
    "lerna:publish:yes": "yarn run prepublish:lockfile && yarn run prepublishOnly:check-bin && npx lerna publish --yes --bump patch",
    "prepublish:lockfile": "ynpx sync-lockfile",
    "prepare:fix-ws-links": "ynpx @yarn-tool/fix-ws-links",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "postpublish:subrepo": "git subrepo push --all & git push",
    "ncu": "yarn run ncu:root && yarn run ncu:ws",
    "ncu:root": "npx yarn-tool ncu -u",
    "ncu:ws": "npx yarn-tool ws exec yarn-tool ncu -- -u",
    "sort-package-json": "yarn run sort-package-json:root && yarn run sort-package-json:ws",
    "sort-package-json:root": "npx yarn-tool sort",
    "sort-package-json:ws": "npx yarn-tool ws exec yarn-tool sort"
  },
  "resolutions": {},
  "dependencies": {
    "array-hyper-unique": "^1.1.9",
    "chalk": "< 3 && >= 2",
    "crlf-normalize": "^1.0.5",
    "debug-color2": "^1.2.3",
    "inquirer": "^8.0.0",
    "package-json": "^6.5.0",
    "pkg-up": "^3.1.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/err-code": "^2.0.0",
    "@bluelovers/fast-glob": "^3.0.4",
    "@bluelovers/tsconfig": "^1.0.20",
    "@types/bluebird": "^3.5.34",
    "@types/chai": "^4.2.18",
    "@types/chalk": "^2.2.0",
    "@types/cli-table": "^0.3.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.5",
    "@types/deep-diff": "^1.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/hash-sum": "^1.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.23",
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.169",
    "@types/micromatch": "^4.0.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.3.0",
    "@types/npm-package-arg": "^6.1.0",
    "@types/npm-packlist": "^1.1.1",
    "@types/parse-git-config": "^3.0.0",
    "@types/semver": "^7.3.6",
    "@types/tar": "^4.0.4",
    "@types/tmp": "^0.2.0",
    "@types/update-notifier": "^5.0.0",
    "@types/validate-npm-package-name": "^3.0.2",
    "@types/yargs": "^16.0.1",
    "@types/yarnpkg__lockfile": "^1.1.4",
    "chai": "^4.3.4",
    "jest": "^26.6.3",
    "mocha": "^8.4.0",
    "ts-jest": "^26.5.6"
  },
  "peerDependencies": {
    "lerna": "^4"
  }
}
