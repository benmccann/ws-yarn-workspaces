{
  "name": "ws-create-yarn-workspaces",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "yarn",
    "yarn-tool",
    "npm",
    "lerna",
    "workspaces",
    "pkg",
    "dependencies",
    "monorepo",
    "yarnlock",
    "node-modules",
    "packages",
    "package"
  ],
  "homepage": "https://github.com/bluelovers/ws-yarn-workspaces#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-yarn-workspaces/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-yarn-workspaces.git"
  },
  "workspaces": [
    "packages/@yarn-tool/*",
    "packages/@lazy-node/*",
    "packages/*"
  ],
  "scripts": {
    "test": "yarn-tool ws run test",
    "lerna:publish": "npx lerna publish",
    "lerna:publish:yes": "yarn run prepublish:lockfile && yarn run prepublishOnly:check-bin && npx lerna publish --yes --bump patch",
    "prepublish:lockfile": "ynpx sync-lockfile",
    "prepare:fix-ws-links": "ynpx @yarn-tool/fix-ws-links",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "postpublish": "yarn run postpublishOnly:ws-root-changelog",
    "postpublish:subrepo": "git subrepo push --all & git push",
    "postpublishOnly:ws-root-changelog": "ynpx ws-root-changelog & git commit ./CHANGELOG.md -m \"chore(changelog): update changelog toc in workspaces root\" & echo update changelog toc in workspaces root",
    "ncu": "yarn run ncu:root && yarn run ncu:ws",
    "ncu:root": "npx yarn-tool ncu -u",
    "ncu:ws": "npx yarn-tool ws exec yarn-tool ncu -- -u",
    "sort-package-json": "yarn run sort-package-json:root && yarn run sort-package-json:ws",
    "sort-package-json:root": "npx yarn-tool sort",
    "sort-package-json:ws": "npx yarn-tool ws exec yarn-tool sort"
  },
  "resolutions": {},
  "dependencies": {
    "array-hyper-unique": "^2.0.8",
    "chalk": "< 3 && >= 2",
    "crlf-normalize": "^1.0.7",
    "debug-color2": "^1.2.7",
    "inquirer": "^9.0.0",
    "jest-console": "^0.1.0",
    "level": "^8.0.0",
    "package-json": "<7 >=6.5",
    "pkg-up": "< 4 >= 3.1.0",
    "semver": "^7.3.7",
    "tslib": "^2"
  },
  "devDependencies": {
    "@bluelovers/fast-glob": "^3.0.4",
    "@bluelovers/tsconfig": "^1.0.30",
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.3.1",
    "@types/chalk": "^2.2.0",
    "@types/cli-table": "^0.3.0",
    "@types/conventional-changelog": "^3.1.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.7",
    "@types/err-code": "^2.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/hash-sum": "^1.0.0",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.3",
    "@types/json5": "^0.0.30",
    "@types/level": "^6.0.1",
    "@types/lodash": "^4.14.182",
    "@types/micromatch": "^4.0.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/npm-package-arg": "^6.1.1",
    "@types/npm-packlist": "^3.0.0",
    "@types/parse-git-config": "^3.0.1",
    "@types/semver": "^7.3.10",
    "@types/tar": "^6.1.1",
    "@types/tmp": "^0.2.3",
    "@types/update-notifier": "^5.1.0",
    "@types/validate-npm-package-name": "^4.0.0",
    "@types/yargs": "^17.0.10",
    "@types/yarnpkg__lockfile": "^1.1.5",
    "chai": "^4.3.6",
    "jest": "^28.1.1",
    "jest-console": "^0.1.0",
    "mocha": "^10.0.0",
    "ts-jest": "^28.0.5"
  },
  "peerDependencies": {
    "lerna": "^5"
  }
}
